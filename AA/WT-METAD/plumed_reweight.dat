dist: READ FILE=COLVAR VALUES=dist IGNORE_FORCES IGNORE_TIME
coord: READ FILE=COLVAR VALUES=coord IGNORE_FORCES IGNORE_TIME
metad: READ FILE=COLVAR VALUES=metad.* IGNORE_FORCES IGNORE_TIME

weights: REWEIGHT_METAD TEMP=298 ARG=metad.rbias

HISTOGRAM ...
  ARG=dist,coord
  GRID_MIN=0,0
  GRID_MAX=25,1000
  GRID_BIN=400,400
  KERNEL=GAUSSIAN
  BANDWIDTH=0.08,0.8
  LOGWEIGHTS=weights
  LABEL=hD
  UPDATE_FROM=200000
... HISTOGRAM

ff: CONVERT_TO_FES GRID=hD TEMP=298
DUMPGRID GRID=ff FILE=new_fes_bw1.dat


ENDPLUMED

